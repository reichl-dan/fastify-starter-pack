{
  "name": "fastify-core",
  "version": "1.0.0",
  "description": "A Fastify plugin that bundles commonly used plugins and utilities for developing a Fastify API",
  "author": "Dan Reichl <reichl.daniel.p@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "keywords": ["fastify", "plugin", "template", "bundle", "api"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reichl-dan/fastify-core"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch test/startServer.ts",
    "prepare": "pnpm run build",
    "lint": "biome lint . --write",
    "format": "biome format . --write",
    "check": "biome check . --write"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/rate-limit": "^10.2.2",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger-ui": "^5.2.2",
    "@fastify/swagger": "^9.4.2",
    "fastify-plugin": "^5.0.1",
    "fastify": "^5.2.1",
    "pino-pretty": "^13.0.0",
    "pino": "^9.6.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "22.13.10",
    "tsx": "4.19.3",
    "typescript": "5.8.2"
  }
}
